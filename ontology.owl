<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xml:base="http://example.org/ontology"
    xmlns="http://example.org/ontology#"
    xmlns:ex="http://example.org/ontology#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <owl:Ontology rdf:about="http://example.org/ontology">
    <rdfs:label>Example Ontology</rdfs:label>
  </owl:Ontology>

  <!-- ===== CLASSES ===== -->
  <owl:Class rdf:about="#Person"/>
  <owl:Class rdf:about="#PersonTrait"/>
  <owl:Class rdf:about="#Occupation"/>
  <owl:Class rdf:about="#Resource"/>
  <owl:Class rdf:about="#Food"/>
  <owl:Class rdf:about="#FoodCategory"/>
  <owl:Class rdf:about="#FoodType"/>
  <owl:Class rdf:about="#Health"/>
  <owl:Class rdf:about="#Condition"/>
  <owl:Class rdf:about="#Disease"/>
  <owl:Class rdf:about="#Symptom"/>
  <owl:Class rdf:about="#Allergy"/>
  <owl:Class rdf:about="#City"/>
  <owl:Class rdf:about="#Landmark"/>
  <owl:Class rdf:about="#Population"/>
  <owl:Class rdf:about="#Facility"/>
  <owl:Class rdf:about="#Protocol"/>
  <owl:Class rdf:about="#Event"/>
  <owl:Class rdf:about="#ServiceEvent">
    <rdfs:subClassOf rdf:resource="#Event"/>
  </owl:Class>
  <owl:Class rdf:about="#QuakeEvent">
    <rdfs:subClassOf rdf:resource="#Event"/>
  </owl:Class>
  <owl:Class rdf:about="#CovidEvent">
    <rdfs:subClassOf rdf:resource="#Event"/>
  </owl:Class>

  <!-- Occupations -->
  <owl:Class rdf:about="#Barista">
    <rdfs:subClassOf rdf:resource="#Occupation"/>
  </owl:Class>
  <owl:Class rdf:about="#Firefighter">
    <rdfs:subClassOf rdf:resource="#Occupation"/>
  </owl:Class>
  <owl:Class rdf:about="#Doctor">
    <rdfs:subClassOf rdf:resource="#Occupation"/>
  </owl:Class>
  <owl:AllDisjointClasses>
    <owl:members rdf:parseType="Collection">
      <rdf:Description rdf:about="#Barista"/>
      <rdf:Description rdf:about="#Firefighter"/>
      <rdf:Description rdf:about="#Doctor"/>
    </owl:members>
  </owl:AllDisjointClasses>

  <!-- Traits -->
  <owl:Class rdf:about="#Arrogant">
    <rdfs:subClassOf rdf:resource="#PersonTrait"/>
  </owl:Class>
  <owl:Class rdf:about="#Polite">
    <rdfs:subClassOf rdf:resource="#PersonTrait"/>
    <owl:disjointWith rdf:resource="#Arrogant"/>
  </owl:Class>
  <owl:Class rdf:about="#Cowardly">
    <rdfs:subClassOf rdf:resource="#PersonTrait"/>
  </owl:Class>
  <owl:Class rdf:about="#Heroic">
    <rdfs:subClassOf rdf:resource="#PersonTrait"/>
    <owl:disjointWith rdf:resource="#Cowardly"/>
  </owl:Class>

  <!-- Resources -->
  <owl:Class rdf:about="#CoffeeMachine"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Water"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Electricity"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Milk"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#AltMilk"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#PPE"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Ventilator"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Firetruck"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#BreathingApparatus"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Hose"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Ambulance"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#RescueTools"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>
  <owl:Class rdf:about="#Comms"><rdfs:subClassOf rdf:resource="#Resource"/></owl:Class>

  <!-- Food -->
  <owl:Class rdf:about="#FlourContaining"><rdfs:subClassOf rdf:resource="#FoodCategory"/></owl:Class>
  <owl:Class rdf:about="#NutContaining"><rdfs:subClassOf rdf:resource="#FoodCategory"/></owl:Class>
  <owl:Class rdf:about="#GlutenFree"><rdfs:subClassOf rdf:resource="#FoodCategory"/></owl:Class>
  <owl:Class rdf:about="#LactoseContaining"><rdfs:subClassOf rdf:resource="#FoodCategory"/></owl:Class>

  <owl:Class rdf:about="#WheatPizza"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>
  <owl:Class rdf:about="#GlutenfreePizza"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>
  <owl:Class rdf:about="#Stroopwafel"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>
  <owl:Class rdf:about="#WheatPancake"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>
  <owl:Class rdf:about="#GlutenfreePancake"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>
  <owl:Class rdf:about="#Coffee"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>
  <owl:Class rdf:about="#Juice"><rdfs:subClassOf rdf:resource="#FoodType"/></owl:Class>

  <!-- Health -->
  <owl:Class rdf:about="#Asthma"><rdfs:subClassOf rdf:resource="#Condition"/></owl:Class>
  <owl:Class rdf:about="#Diabetes"><rdfs:subClassOf rdf:resource="#Condition"/></owl:Class>
  <owl:Class rdf:about="#ProteinDeficiency"><rdfs:subClassOf rdf:resource="#Condition"/></owl:Class>
  <owl:Class rdf:about="#Covid19"><rdfs:subClassOf rdf:resource="#Disease"/></owl:Class>

  <owl:Class rdf:about="#Coughing"><rdfs:subClassOf rdf:resource="#Symptom"/></owl:Class>
  <owl:Class rdf:about="#Fever"><rdfs:subClassOf rdf:resource="#Symptom"/></owl:Class>
  <owl:Class rdf:about="#Pain"><rdfs:subClassOf rdf:resource="#Symptom"/></owl:Class>
  <owl:Class rdf:about="#Dyspnea"><rdfs:subClassOf rdf:resource="#Symptom"/></owl:Class>

  <owl:Class rdf:about="#AllergyGluten"><rdfs:subClassOf rdf:resource="#Allergy"/></owl:Class>
  <owl:Class rdf:about="#AllergyLactose"><rdfs:subClassOf rdf:resource="#Allergy"/></owl:Class>
  <owl:Class rdf:about="#AllergyNut"><rdfs:subClassOf rdf:resource="#Allergy"/></owl:Class>

  <!-- Population -->
  <owl:Class rdf:about="#Crowded"><rdfs:subClassOf rdf:resource="#Population"/></owl:Class>
  <owl:Class rdf:about="#Sparse">
    <rdfs:subClassOf rdf:resource="#Population"/>
    <owl:disjointWith rdf:resource="#Crowded"/>
  </owl:Class>

  <!-- Facilities -->
  <owl:Class rdf:about="#Hospital"><rdfs:subClassOf rdf:resource="#Facility"/></owl:Class>
  <owl:Class rdf:about="#Firestation"><rdfs:subClassOf rdf:resource="#Facility"/></owl:Class>
  <owl:Class rdf:about="#Cafe"><rdfs:subClassOf rdf:resource="#Facility"/></owl:Class>

  <!-- Protocols -->
  <owl:Class rdf:about="#BaristaServiceProtocol"><rdfs:subClassOf rdf:resource="#Protocol"/></owl:Class>
  <owl:Class rdf:about="#EarthquakeResponseProtocol"><rdfs:subClassOf rdf:resource="#Protocol"/></owl:Class>
  <owl:Class rdf:about="#CovidIsolationProtocol"><rdfs:subClassOf rdf:resource="#Protocol"/></owl:Class>

  <!-- ===== OBJECT PROPERTIES ===== -->
  <owl:ObjectProperty rdf:about="#friendWith">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#marriedTo">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#locatedIn">
    <rdfs:domain rdf:resource="#Event"/>
    <rdfs:range rdf:resource="#City"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#occursBefore">
    <rdfs:domain rdf:resource="#Event"/>
    <rdfs:range rdf:resource="#Event"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasOccupation">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Occupation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasLandmark">
    <rdfs:domain rdf:resource="#City"/>
    <rdfs:range rdf:resource="#Landmark"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#landmarkOf">
    <rdfs:domain rdf:resource="#Landmark"/>
    <rdfs:range rdf:resource="#City"/>
    <owl:inverseOf rdf:resource="#hasLandmark"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasTrait">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#PersonTrait"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#worksAt">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Facility"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#livesIn">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#City"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#requiresResource">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#Occupation"/>
          <rdf:Description rdf:about="#Protocol"/>
          <rdf:Description rdf:about="#Event"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Resource"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#followsProtocol">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Protocol"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#appliesTo">
    <rdfs:domain rdf:resource="#Protocol"/>
    <rdfs:range rdf:resource="#Disease"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#partOfCategory">
    <rdfs:domain rdf:resource="#FoodType"/>
    <rdfs:range rdf:resource="#FoodCategory"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#allergicTo">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#FoodCategory"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#famousForFood">
    <rdfs:domain rdf:resource="#City"/>
    <rdfs:range rdf:resource="#FoodType"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasPopulation">
    <rdfs:domain rdf:resource="#City"/>
    <rdfs:range rdf:resource="#Population"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#involves">
    <rdfs:domain rdf:resource="#Event"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#Person"/>
          <rdf:Description rdf:about="#Facility"/>
          <rdf:Description rdf:about="#City"/>
          <rdf:Description rdf:about="#Resource"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#admittedAt">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Hospital"/>
  </owl:ObjectProperty>

  <!-- Disease -> Symptom -->
  <owl:ObjectProperty rdf:about="#hasSymptom">
    <rdfs:domain rdf:resource="#Disease"/>
    <rdfs:range rdf:resource="#Symptom"/>
  </owl:ObjectProperty>

  <!-- ===== DATA PROPERTIES ===== -->
  <owl:DatatypeProperty rdf:about="#isWalkable">
    <rdfs:domain rdf:resource="#City"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#isDrivable">
    <rdfs:domain rdf:resource="#City"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#hasAge">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#builtYear">
    <rdfs:domain rdf:resource="#Landmark"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#hasMagnitude">
    <rdfs:domain rdf:resource="#QuakeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:DatatypeProperty>

</rdf:RDF>
